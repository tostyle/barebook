{
  "name": "template-api",
  "version": "0.1.0",
  "description": "Template API",
  "author": "R&D Backend Team",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=7.10.0",
    "npm": ">=4.2.0",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon index.js",
    "start:debug": "cross-env DEBUG=template-api:* yarn start",
    "lint": "standard",
    "lint:watch": "npm run lint -- --watch",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server/tests --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server/tests --recursive",
    "test:check-coverage": "npm run test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tostyle"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "ejs": "2.5.7",
    "express": "4.14.0",
    "express-jwt": "5.1.0",
    "express-validation": "1.0.1",
    "express-winston": "2.1.2",
    "gulp": "3.9.1",
    "gulp-sass": "3.1.0",
    "helmet": "3.1.0",
    "http-status": "^0.2.0",
    "joi": "10.0.6",
    "method-override": "^2.3.5",
    "mongoose": "4.7.4",
    "morgan": "1.7.0",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "commitizen": "^2.9.2",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.3",
    "cz-conventional-changelog": "1.2.0",
    "del": "^2.2.0",
    "husky": "^0.13.3",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.2.0",
    "run-sequence": "^1.1.5",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.6.1"
  },
  "license": "MIT"
}
